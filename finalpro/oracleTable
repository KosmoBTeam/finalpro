--food 테이블
create table food(
num number(5),
locnum number(5),
title varchar2(50),
img varchar2(50),
address varchar2(100),
tel varchar2(15),
detail varchar2(500),
constraint food_num_pk primary key(num),
constraint food_locnum_fk foreign key(locnum) references local(locnum));
--food 시퀀스
create sequence food_seq
increment by 1
start with 1;
--food 데이터

--act 테이블
create table act(
num number(5),
locnum number(5),
title varchar2(50),
img varchar2(50),
address varchar2(100),
tel varchar2(15),
detail varchar2(500),
constraint act_num_pk primary key(num),
constraint act_locnum_fk foreign key(locnum) references local(locnum));
--act 시퀀스
create sequence act_seq
increment by 1
start with 1;
