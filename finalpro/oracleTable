--hotel 테이블
create table hotel(
num number(5),
locnum number(5),
title varchar2(50),
img varchar2(50),
address varchar2(100),
tel varchar2(15),
detail varchar2(500),
constraint hotel_num_pk primary key(num),
constraint hotel_locnum_fk foreign key(locnum) references local(locnum));
--hotel 시퀀스

create sequence hotel_seq
increment by 1
start with 2;
--hotel 데이터

